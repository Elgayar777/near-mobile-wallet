{
    "name": "mobile-base-project",
    "private": true,
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "yarn generate-icons && yarn generate-images && expo start",
        "android": "yarn start --android",
        "ios": "yarn start --ios",
        "web": "yarn start --web",
        "generate-icons": "node ./src/script/iconGeneratorNative.js ./src/asset/icon/ ./src/module/common/icons/ && prettier --write ./src/module/common/icons/",
        "generate-images": "node ./src/script/imagesGenerator.js ./src/asset/image/ && prettier --write ./src/asset/image/",
        "openapi:generate": "openapi -i openapi-spec.json -o src/module/api/service --exportSchemas=true --useUnionTypes",
        "eject": "expo eject",
        "test:unit": "jest --coverage",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json --max-warnings 0"
    },
    "dependencies": {
        "@peersyst/react-hooks": "^1.1.2",
        "@peersyst/react-types": "^1.0.0",
        "@peersyst/react-utils": "^1.0.0",
        "@react-native-async-storage/async-storage": "^1.15.16",
        "@react-navigation/bottom-tabs": "^6.0.5",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/native-stack": "^6.1.0",
        "dotenv": "^14.2.0",
        "expo": "~44.0.0",
        "expo-asset": "~8.4.4",
        "expo-constants": "~13.0.0",
        "expo-font": "~10.0.4",
        "expo-linking": "~3.0.0",
        "expo-secure-store": "^11.1.0",
        "expo-splash-screen": "~0.14.0",
        "expo-status-bar": "~1.2.0",
        "expo-web-browser": "~10.1.0",
        "i18n-js": "^3.8.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "0.64.3",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.10.1",
        "react-native-svg": "^12.1.1",
        "react-native-web": "0.17.1",
        "react-query": "^3.34.11",
        "recoil": "^0.5.2",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@testing-library/jest-native": "^4.0.4",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/react-native": "^9.0.0",
        "@types/i18n-js": "^3.8.2",
        "@types/jest": "^27.4.0",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.64.12",
        "@types/styled-components": "^5.1.20",
        "@types/styled-components-react-native": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jest": "^27.4.7",
        "jest-expo": "^44.0.1",
        "openapi-typescript": "^5.1.0",
        "openapi-typescript-codegen": "^0.13.0",
        "prettier": "^2.5.1",
        "react-test-renderer": "17.0.1",
        "stylelint": "^14.3.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "ts-jest": "^27.1.3",
        "typescript": "~4.3.5"
    }
}
